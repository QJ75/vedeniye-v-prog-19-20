    if (A==B) {
        A=B=0;
        cout<<"При изменении А и В ="<<A<<endl;
    }
    else {
        A=B=max(A,B);
        cout<<"При изменении А и В ="<<A<<endl;
    }
    
    
    
    return 0;
}
