/1/
#include <stdio.h>
 
int main(void)
{
    int N;
    printf("N:");
    scanf ("%i", &N);
 
    printf("%i\n",N%60);
 
    return 0;
}
/2/
#include <stdio.h>
 
int main(void)
{
    int K;
    printf("K:");
    scanf ("%i", &K);
 
    printf("%i\n",K%7);
 
    return 0;
}
/3/
#include <stdio.h>
 
int main(void)
{
    int K,N;
    printf("K:");
    scanf ("%i", &K);
 
    printf("N:");
    scanf ("%i", &N);
 
 
    printf("%i\n",((K+N-2)%7)+1);
 
    return 0;
}
/4/
#include <stdio.h>
 
int main(void)
{
    int A,B,C;
    printf("A:");
    scanf ("%i", &A);
 
    printf("B:");
    scanf ("%i", &B);
 
    printf("C:");
    scanf ("%i", &C);
 
    printf("%i\n",(A/C)*(B/C));
 
    return 0;
}
/5/
#include <stdio.h>
 
int main(void)
{
    int year;
    printf("Год:");
    scanf ("%i", &year);
 
    printf("%i\n",((year-1)/100)+1);
 
    return 0;
}
